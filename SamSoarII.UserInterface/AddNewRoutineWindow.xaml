<Window x:Uid="Window_1" x:Class="SamSoarII.UserInterface.AddNewRoutineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SamSoarII.UserInterface"
        xmlns:properties="clr-namespace:SamSoarII.UserInterface.Properties"
        mc:Ignorable="d"
        Title="{x:Static properties:Resources.AddNewSubRoutine}" Height="300" Width="300">
    
    <Canvas x:Uid="Canvas_1">
        <Label x:Uid="Label_1" Content="{x:Static properties:Resources.SubRoutine_Name}" Canvas.Left="22" Canvas.Top="86"></Label>
        <TextBox x:Uid="NameTextBox" x:Name="NameTextBox" Width="150" Height="25" Canvas.Left="95" Canvas.Top="86"></TextBox>
        <Button x:Uid="EnsureButton" x:Name="EnsureButton" Content="{x:Static properties:Resources.Ensure}" Width="70" Height="25" Canvas.Left="34" Canvas.Top="184"/>
        <Button x:Uid="CancelButton" x:Name="CancelButton" Content="{x:Static properties:Resources.Cancel}" Width="70" Height="25" Canvas.Left="175" Canvas.Top="184"/>
    </Canvas>
</Window>
