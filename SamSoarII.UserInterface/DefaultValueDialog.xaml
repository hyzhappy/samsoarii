<Window x:Uid="Window_1" x:Class="SamSoarII.UserInterface.DefaultValueDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SamSoarII.UserInterface"
        mc:Ignorable="d"
        Title="DefaultValueDialog" Height="180" Width="300">
    <Grid x:Uid="Grid_1">
        <Canvas x:Uid="Canvas_1">
            <TextBlock x:Uid="TextBlock_1" Text="是否恢复默认值?" FontSize="20" Canvas.Left="70" Canvas.Top="30"></TextBlock>
            <Button x:Uid="EnsureButton" x:Name="EnsureButton" Content="确定" Width="70" Height="25" Canvas.Left="34" Canvas.Top="100"/>
            <Button x:Uid="CancelButton" x:Name="CancelButton" Content="取消" Width="70" Height="25" Canvas.Left="175" Canvas.Top="100"/>
        </Canvas>
    </Grid>
</Window>
