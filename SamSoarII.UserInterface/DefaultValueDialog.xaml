<Window x:Uid="Window_1" x:Class="SamSoarII.UserInterface.DefaultValueDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SamSoarII.UserInterface"
        xmlns:properties="clr-namespace:SamSoarII.UserInterface.Properties"
        mc:Ignorable="d"
        Title="{x:Static properties:Resources.Default}" Height="180" Width="300">
    <Grid x:Uid="Grid_1">
        <Canvas x:Uid="Canvas_1">
            <TextBlock TextWrapping="Wrap" Width="210" Text="{x:Static properties:Resources.Message_Default}" FontSize="16" Canvas.Left="35" Canvas.Top="30"></TextBlock>
            <Button x:Name="EnsureButton" Content="{x:Static properties:Resources.Ensure}" Width="70" Height="20" Canvas.Left="34" Canvas.Top="100"/>
            <Button x:Name="CancelButton" Content="{x:Static properties:Resources.Cancel}" Width="70" Height="20" Canvas.Left="175" Canvas.Top="100"/>
        </Canvas>
    </Grid>
</Window>
