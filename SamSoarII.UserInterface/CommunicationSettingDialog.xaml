<Window x:Class="SamSoarII.UserInterface.CommunicationSettingDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SamSoarII.UserInterface"
        mc:Ignorable="d"
        Title="通信设置" Height="250" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <local:BaseSetting x:Name="baseSetting"></local:BaseSetting>
        <StackPanel Grid.Row="2" Orientation="Horizontal">
            <Button x:Name="CommunicationTestButton" Width="60" Height="20" Margin="5,0,0,0">通信测试</Button>
            <Button x:Name="EnsureButton" Width="50" Height="20" Margin="200,0,0,0">确定</Button>
            <Button x:Name="CancelButton" Width="50" Height="20" Margin="10,0,0,0">取消</Button>
        </StackPanel>
    </Grid>
</Window>
