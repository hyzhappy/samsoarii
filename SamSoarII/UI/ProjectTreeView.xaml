                    <local:InstrucionTreeItem InstructionIndex="1809" Header="NEG()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1810" Header="NEGD()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1811" Header="XCH()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1812" Header="XCHD()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1813" Header="XCHF()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1814" Header="CML()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1815" Header="CMLD()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1816" Header="SMOV()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1817" Header="FMOV()"></local:InstrucionTreeItem>
                    <local:InstrucionTreeItem InstructionIndex="1818" Header="FMOVD()"></local:InstrucionTreeItem>
                </TreeViewItem>
            </TreeViewItem>
        </TreeView>
    </Grid> 
</UserControl>
