        <Canvas Width="30" x:Name="canvas" Height="24">
            <Label>
                <Path Stroke="Black" x:Name="path">
                    <Path.Data>
                        <PathGeometry>
                            <PathFigure StartPoint="0,5">
                                <LineSegment Point="20,5"></LineSegment>
                            </PathFigure>
                        </PathGeometry>
                    </Path.Data>
                </Path>
            </Label>
        </Canvas>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="canvas" Property="Background" Value="#FFA5D6FF"></Setter>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="path" Property="StrokeThickness" Value="1.3"></Setter>
            </Trigger>
            <Trigger Property="IsEnabled" Value="false">
                <Setter TargetName="path" Property="Opacity" Value="0.3"></Setter>
