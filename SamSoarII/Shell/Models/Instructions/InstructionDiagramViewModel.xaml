<UserControl x:Class="SamSoarII.Shell.Models.InstructionDiagramViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Shell.Models"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="800"
             KeyDown="OnKeyDown">
    <ScrollViewer x:Uid="Scroll" x:Name="Scroll" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" SizeChanged="Scroll_SizeChanged">
        <StackPanel x:Uid="MainStack" x:Name="MainStack" Orientation="Vertical"/>
    </ScrollViewer>
</UserControl>
