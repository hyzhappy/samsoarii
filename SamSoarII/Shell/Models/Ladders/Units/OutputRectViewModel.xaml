<local:LadderUnitViewModel x:Class="SamSoarII.Shell.Models.OutputRectViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Shell.Models"
             xmlns:mng="clr-namespace:SamSoarII.Shell.Managers"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Background="Transparent">
    <Canvas x:Name="mainCanvas">
        <Border Canvas.Left="20" Canvas.Top="20" Width="280" Height="280" 
                    BorderThickness="4" BorderBrush="Black">
        </Border>
        <Line X1="0" Y1="100" X2="20" Y2="100" 
                  Stroke="Black" StrokeThickness="4">
        </Line>
        <Label x:Name="LB_EN" Canvas.Left="30" Canvas.Top="80" FontSize="25">
            EN
        </Label>
        <TextBlock x:Name="TopTextBlock" 
                       Canvas.Left="25" Canvas.Top="20" TextAlignment="Center">
        </TextBlock>
    </Canvas>
</local:LadderUnitViewModel>
