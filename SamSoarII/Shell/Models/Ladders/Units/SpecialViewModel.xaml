<local:LadderUnitViewModel x:Class="SamSoarII.Shell.Models.SpecialViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Shell.Models"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="300" Height="300" Background="Transparent">
    <StackPanel>
        <Canvas x:Name="mainCanvas" Width="300" Height="300">
            <Line X1="0" Y1="100" X2="300" Y2="100" Stroke="Black" StrokeThickness="4">
            </Line>
            <Canvas x:Name="CenterCanvas" Width="100" Height="100" Canvas.Left="100" Canvas.Top="50">
            </Canvas>
        </Canvas>
        <StackPanel Width="300" Height="200">
        </StackPanel>
    </StackPanel>
</local:LadderUnitViewModel>
