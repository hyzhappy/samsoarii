<UserControl x:Class="SamSoarII.Shell.Models.NetworkOutlineViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Shell.Models"
             mc:Ignorable="d" 
             d:DesignHeight="385" d:DesignWidth="800" MaxHeight="385">
    <ScrollViewer x:Name="Scroll" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <local:LadderCanvas x:Name="LadderCanvas" Background="White" HorizontalAlignment="Left">
            <local:LadderCanvas.LayoutTransform>
                <ScaleTransform x:Name="LadderScale"></ScaleTransform>
            </local:LadderCanvas.LayoutTransform>
        </local:LadderCanvas>
    </ScrollViewer>
</UserControl>
