<base:SimuViewTabModel x:Class="SamSoarII.Simulation.UI.SimuViewAllDiaModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.UI"
             xmlns:base="clr-namespace:SamSoarII.Simulation.UI.Base"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="3000">
    <UserControl.Content>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <ScrollViewer.Content>
                <StackPanel x:Name="DiagramPanel">
                    <Expander x:Name="MainRoutineExpander" Focusable="False">
                        <Expander.Header>
                            <TextBlock FontSize="16">主程序</TextBlock>
                        </Expander.Header>
                    </Expander>
                </StackPanel>
            </ScrollViewer.Content>
        </ScrollViewer>
    </UserControl.Content>
</base:SimuViewTabModel>
