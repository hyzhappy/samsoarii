<UserControl x:Uid="UserControl_1" x:Class="SamSoarII.Simulation.UI.Chart.TimeRuler"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.UI.Chart"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="800">
    <UserControl.Content>
        <Canvas x:Uid="MainCanva" x:Name="MainCanva" Background="DarkBlue">
            
        </Canvas>
    </UserControl.Content>
    <UserControl.ContextMenu>
        <ContextMenu x:Uid="ContextMenu_1">
            <MenuItem x:Uid="MI_StartPoint" x:Name="MI_StartPoint" Header="放置起点" Click="MI_StartPoint_Click"/>
            <MenuItem x:Uid="MI_EndPoint" x:Name="MI_EndPoint" Header="放置终点" Click="MI_EndPoint_Click"/>
            <Separator x:Uid="Separator_1"/>
            <MenuItem x:Uid="MI_Setting" x:Name="MI_Setting" Header="设置" Click="MI_Setting_Click"/>
        </ContextMenu>
    </UserControl.ContextMenu>
</UserControl>
