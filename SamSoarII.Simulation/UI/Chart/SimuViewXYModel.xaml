<base:SimuViewTabModel x:Uid="base:SimuViewTabModel_1" x:Class="SamSoarII.Simulation.UI.Chart.SimuViewXYModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.UI.Chart"
             xmlns:base="clr-namespace:SamSoarII.Simulation.UI.Base"
             mc:Ignorable="d" 
             d:DesignHeight="632" d:DesignWidth="832">
    <Grid x:Uid="Grid_1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Uid="ColumnDefinition_1" Width="32"/>
            <ColumnDefinition x:Uid="ColumnDefinition_2" Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="*"/>
            <RowDefinition x:Uid="RowDefinition_2" Height="32"/>
        </Grid.RowDefinitions>
        <local:TimeRuler x:Uid="XRuler" x:Name="XRuler" Grid.Row="1" Grid.Column="1"/>
        <local:ValueRuler x:Uid="YRuler" x:Name="YRuler" Grid.Row="0" Grid.Column="0"/>
        <Canvas x:Uid="MainCanva" x:Name="MainCanva" Grid.Row="0" Grid.Column="1" Background="AliceBlue"/>
    </Grid>
</base:SimuViewTabModel>
