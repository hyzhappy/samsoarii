<base:SimuViewTabModel x:Class="SamSoarII.Simulation.UI.Chart.SimuViewXYModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.UI.Chart"
             xmlns:base="clr-namespace:SamSoarII.Simulation.UI.Base"
             mc:Ignorable="d" 
             d:DesignHeight="632" d:DesignWidth="832">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="32"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="32"/>
        </Grid.RowDefinitions>
        <local:TimeRuler x:Name="XRuler" Grid.Row="1" Grid.Column="1"/>
        <local:ValueRuler x:Name="YRuler" Grid.Row="0" Grid.Column="0"/>
        <Canvas x:Name="MainCanva" Grid.Row="0" Grid.Column="1" Background="AliceBlue"/>
    </Grid>
</base:SimuViewTabModel>
