<Window x:Uid="Window_1" x:Class="SamSoarII.Simulation.UI.Chart.Window.TimePointWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.UI.Chart.Window"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="300"
              Width="300" Height="60">
    <Grid x:Uid="Grid_1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Uid="ColumnDefinition_1" Width="2*"/>
            <ColumnDefinition x:Uid="ColumnDefinition_2" Width="1*"/>
            <ColumnDefinition x:Uid="ColumnDefinition_3" Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.Children>
            <TextBox x:Uid="TB_Time" x:Name="TB_Time" Grid.Row="0" Grid.Column="0"/>
            <ComboBox x:Uid="CB_TimeUnit" x:Name="CB_TimeUnit" Grid.Row="0" Grid.Column="1" Text="ms">
                <ComboBox.Items>
                    <TextBlock x:Uid="TextBlock_1" FontSize="16">ms</TextBlock>
                    <TextBlock x:Uid="TextBlock_2" FontSize="16">s</TextBlock>
                </ComboBox.Items>
            </ComboBox>
            <Button x:Uid="B_ok" x:Name="B_ok" Grid.Row="0" Grid.Column="2">确定</Button>
        </Grid.Children>
    </Grid>
</Window>
