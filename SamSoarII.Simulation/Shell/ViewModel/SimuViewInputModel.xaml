<local:SimuViewBaseModel x:Uid="local:SimuViewBaseModel_1" x:Class="SamSoarII.Simulation.Shell.ViewModel.SimuViewInputModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.Simulation.Shell.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas x:Uid="Canvas_1" Width="300" Height="300" Background="Transparent">
        <Line x:Uid="Line_1" X1="0" Y1="100" X2="100" Y2="100" Stroke="Black" StrokeThickness="4"></Line>
        <Line x:Uid="Line_2" X1="200" Y1="100" X2="300" Y2="100" Stroke="Black" StrokeThickness="4"></Line>
        <Line x:Uid="Line_3" X1="100" Y1="50" X2="100" Y2="150" Stroke="Black" StrokeThickness="4"></Line>
        <Line x:Uid="Line_4" X1="200" Y1="50" X2="200" Y2="150" Stroke="Black" StrokeThickness="4"></Line>
        <TextBlock x:Uid="ValueTextBlock" x:Name="ValueTextBlock"  Canvas.Top="10" Canvas.Left="0" Height="40" Width="300" TextAlignment="Center" FontSize="40" Foreground="BlueViolet"></TextBlock>
        <Canvas x:Uid="CenterCanvas" x:Name="CenterCanvas" Canvas.Top="50" Canvas.Left="100" Width="100" Height="100">
            <TextBlock x:Uid="CenterTextBlock" x:Name="CenterTextBlock" Width="100" Canvas.Top="20" FontSize="35" TextAlignment="Center">

            </TextBlock>
        </Canvas>
        <TextBlock x:Uid="Value2TextBlock" x:Name="Value2TextBlock"  Canvas.Top="150" Canvas.Left="0" Height="40" Width="300" TextAlignment="Center" FontSize="40"></TextBlock>
    </Canvas>
</local:SimuViewBaseModel>
