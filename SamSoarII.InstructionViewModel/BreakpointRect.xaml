<UserControl x:Class="SamSoarII.LadderInstViewModel.BreakpointRect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SamSoarII.LadderInstViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas>
        <Rectangle Width="300" Height="300" 
                   Stroke="#FF600808" StrokeThickness="2">
        </Rectangle>
        <Rectangle Canvas.Left="7" Canvas.Top="7" Width="286" Height="286"
                   Stroke="#FF600808" StrokeThickness="2">
        </Rectangle>
        <Rectangle Canvas.Left="252" Width="48" Height="48"
                   Stroke="#FF600808" StrokeThickness="2">
        </Rectangle>
        <Rectangle Canvas.Left="2" Canvas.Top="2" Width="296" Height="296"
                   Stroke="Red" StrokeThickness="5">
        </Rectangle>
        <Rectangle Canvas.Left="254" Canvas.Top="2" Width="44" Height="44"
                   Fill="Red">
        </Rectangle>
        <TextBlock Canvas.Left="258" Canvas.Top="6" Width="36" Height="36"
                   x:Name="TB_Label"
                   Background="White" FontSize="24" TextBlock.TextAlignment="Center">
        </TextBlock>
    </Canvas>
</UserControl>
